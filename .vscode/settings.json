{
    "C_Cpp.intelliSenseEngine": "Disabled",
    "C_Cpp.autocomplete": "Disabled", // So you don't get autocomplete from both extensions.
    "C_Cpp.errorSquiggles": "Disabled", // So you don't get error squiggles from both extensions (clangd's seem to be more reliable anyway).
    "clangd.path": "/usr/bin/clangd",
    "clangd.arguments": [
        "-log=verbose",
        "-pretty",
        "--background-index",
        "--compile-commands-dir=${workspaceFolder}/build-vscode/compile_commands.json"
    ],
    "C_Cpp.default.configurationProvider": "go2sh.cmake-integration",
    "clang-format.executable": "/usr/bin/clang-format",
    "cmake.generator": "Unix Makefiles",
    "cmake.preferredGenerators": [
        "Unix Makefiles"
    ],
    "cmake.buildDirectory": "${workspaceFolder}/build-vscode/${buildKit}/${buildType}",
    "cmake.copyCompileCommands": "${workspaceFolder}/build-vscode/compile_commands.json",
    "cmake.configureSettings": {
        "CMAKE_EXPORT_COMPILE_COMMANDS": 1,
        "DICE_TS_OUTPUT_PROFILE": true,
        "DICE_TS_PROFILE": true,
        "BUILD_GRAPHTRAVERSAL_TEST": false,
        "BUILD_SIMPLE_TEST": false
    },
    "files.associations": {
        "*.ndjson": "jsonl",
        "*.dbclient-js": "javascript",
        "fstream": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "chrono": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "condition_variable": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
